{
    "name": "react-graphql-challenge",
    "version": "1.0.0",
    "description": "AIBS App Team Coding Challenge",
    "private": true,
    "contributors": [
        {
            "name": "Lane Sawyer",
            "email": "lane.sawyer@alleninstitute.org"
        },
        {
            "name": "James Gerstenberger",
            "email": "james.gerstenberger@alleninstitute.org"
        }
    ],
    "browserslist": "> 0.2%",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "start": "parcel index.html --open --port 8080",
        "build": "parcel build index.html --no-scope-hoist",
        "lint": "eslint src --ext .ts,.tsx",
        "test": "jest --config=jest.config.js --watch",
        "format:write": "npx prettier --write .",
        "format:check": "npx prettier --check .",
        "graphql-codegen-types": "graphql-codegen --config codegen.yml"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AllenInstitute/react-graphql-challenge.git"
    },
    "homepage": "https://github.com/AllenInstitute/react-graphql-challenge#readme",
